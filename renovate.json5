{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    ":dependencyDashboard",
    ":maintainLockFilesWeekly",
    ":prHourlyLimitNone",
  ],
  prConcurrentLimit: 0,
  lockFileMaintenance: {
    enabled: true,
  },
  ignoreDeps: ["pytest-cov", "pytest", "pre-commit", "starlette", "mypy", "mkdocs-material", "SQLAlchemy", "mkdocs"]
}
